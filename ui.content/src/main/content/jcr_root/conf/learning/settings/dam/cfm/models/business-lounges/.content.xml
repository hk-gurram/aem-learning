<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Template"
          allowedPaths="[/content/dam/lot-polish-airlines(/.*)?]"
          ranking="{Long}100">
    <jcr:content
        cq:scaffolding="/conf/lot-polish-airlines/settings/dam/cfm/models/business-lounges/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:description="Content fragment model for business lounges"
        jcr:primaryType="cq:PageContent"
        jcr:title="Business lounges"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default">
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="27">
                        <airport
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Airport"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="airport"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <fqtvStatus
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="FQTV Status"
                            listOrder="21"
                            maxlength="255"
                            metaType="text-single"
                            name="fqtvStatus"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <location
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Location"
                            listOrder="22"
                            maxlength="255"
                            metaType="text-single"
                            name="location"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <openingHours
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Opening hours"
                            listOrder="23"
                            maxlength="255"
                            metaType="text-single"
                            name="openingHours"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <wifiAccess
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="25"
                            metaType="boolean"
                            name="wifiAccess"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Wifi access"
                            valueType="boolean" />
                        <computerWorkstation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="26"
                            metaType="boolean"
                            name="computerWorkstation"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Computer workstation"
                            valueType="boolean" />
                        <invitationRequirement
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="27"
                            metaType="boolean"
                            name="invitationRequirement"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Invitation requirement"
                            valueType="boolean" />
                    </items>
                </content>
            </cq:dialog>
        </model>
    </jcr:content>
</jcr:root>
