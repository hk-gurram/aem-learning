<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Template"
          allowedPaths="[/content/dam/lot-polish-airlines(/.*)?]"
          ranking="{Long}100">
    <jcr:content
        cq:scaffolding="/conf/lot-polish-airlines/settings/dam/cfm/models/offices/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:description="Content fragment model for offices"
        jcr:primaryType="cq:PageContent"
        jcr:title="Offices"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default">
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="31">
                        <country
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Country"
                            listOrder="21"
                            maxlength="255"
                            metaType="text-single"
                            name="country"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <city
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="City"
                            listOrder="22"
                            maxlength="255"
                            metaType="text-single"
                            name="city"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <officeName
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Office Name"
                            listOrder="23"
                            maxlength="255"
                            metaType="text-single"
                            name="officeName"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <type
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Type"
                            listOrder="24"
                            maxlength="255"
                            metaType="text-single"
                            name="type"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <telephone
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Telephone"
                            listOrder="25"
                            metaType="text-single"
                            name="telephone"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <openingHours
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Opening Hours"
                            listOrder="26"
                            maxlength="255"
                            metaType="text-single"
                            name="openingHours"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <address
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Address"
                            listOrder="27"
                            maxlength="255"
                            metaType="text-single"
                            name="address"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <postalCode
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Postal Code"
                            listOrder="28"
                            maxlength="255"
                            metaType="text-single"
                            name="postalCode"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <airport
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Airport"
                            listOrder="29"
                            maxlength="255"
                            metaType="text-single"
                            name="airport"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <email
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Email"
                            listOrder="30"
                            maxlength="255"
                            metaType="text-single"
                            name="email"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <fax
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Fax"
                            listOrder="31"
                            metaType="text-single"
                            name="fax"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                        <contactPerson
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Contact Person"
                            listOrder="32"
                            maxlength="255"
                            metaType="text-single"
                            name="contactPerson"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string" />
                    </items>
                </content>
            </cq:dialog>
        </model>
    </jcr:content>
</jcr:root>
